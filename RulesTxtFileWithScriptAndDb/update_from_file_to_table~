do
	sed -i 's/"/ /g' ${filenametxt[$i]}
	txtfile=${filenametxt[$i]}
	htmlfile=${filenamehtml[$i]}
	value=`cat $txtfile`
	sqlite3 usaf_rules "update rules set content=\"$value\" where path=\"$htmlfile\";"
	echo "$htmlfile"
done
